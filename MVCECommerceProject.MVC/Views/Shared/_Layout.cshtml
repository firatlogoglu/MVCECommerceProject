<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -- E-Ticaret</title>
    <!-- Custom fonts for this template-->
    @Styles.Render("~/Content/Sb_Admin_2")
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")
</head>
<body id="page-top">
    <div id="wrapper">
        @Html.Partial("PartialViews/_Sidebar")
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                @Html.Partial("PartialViews/_TopNavbar")
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            @Html.Partial("PartialViews/_Footer")
        </div>
    </div>

    @Html.Partial("PartialViews/_PageTop")

    @Scripts.Render("~/Content/Sb_Admin")
    @RenderSection("scripts", required: false)
</body>
</html>